@charset "UTF-8";

#header{z-index: 98;}

.col-6 label{height: 10px;}

.popup {display:none; position: fixed; left: 50%; top: 50%; z-index: 100; padding: 100px 60px 60px; width: 820px; 
	height: 833px; transform: translate(-50%, -50%); box-sizing: border-box; background: #fff; overflow: auto;}

.popup .close {position: absolute; right: 40px; top:40px; width: 50px; height: 50px; background: url("../../images/ee.png") no-repeat; color: transparent;}

.popup1 {display:none; position: fixed; left: 50%; top: 50%; z-index: 100; padding: 100px 60px 60px; width: 820px; 
	height: 833px; transform: translate(-50%, -50%); box-sizing: border-box; background: #fff; overflow: auto;}

