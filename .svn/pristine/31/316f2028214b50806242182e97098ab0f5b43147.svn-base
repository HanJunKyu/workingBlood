.dim {display:none;  position: fixed; left: 0; top: 0;  width: 100%; height: 100%; z-index: 99;
	background: rgba(0,0,0,0.6);} 

.wrapper {
	margin: 0 auto;
	width: 30%;
}

.major {
	text-align: center;
}

h3:after, h2:after {
	border-bottom: none;
}

h2 {
	line-height: 0px;
	margin: 0;
	padding: 0;
}

.button {
	height: 1.75em;
	line-height: 1.75em;
}

.col-6 {
	height: 50px;
}
